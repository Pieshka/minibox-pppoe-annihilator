# Loading addresses
setenv kernel_addr_r 0x03200000
setenv fdt_addr_r    0x01f00000

# Boot arguments
setenv bootargs "root=/dev/mmcblk0p2 ro rootwait fsck.mode=force fsck.repair=yes console=ttyS2,1500000n8"

# Info
echo "Starting Minibox PPPoE Annihilator"

# Load kernel and FDT
fatload mmc 1:1 ${kernel_addr_r} Image
fatload mmc 1:1 ${fdt_addr_r} rk3328-nanopi-r2s.dtb

# Boot kernel
booti ${kernel_addr_r} - ${fdt_addr_r}
